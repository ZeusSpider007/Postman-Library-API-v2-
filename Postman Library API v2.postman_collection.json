					"script": {
						"exec": [
							"// save the \"id\" value from the response to a variable named \"id\"\r",
							"const id = pm.response.json().id\r",
							"// save the id as a collection variable named \"id\"\r",
							"pm.collectionVariables.set(\"id\", id)"
